{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "denisdums/columnsblock",
  "version": "0.1.0",
  "title": "Columns Block",
  "category": "denisdums",
  "icon": "align-full-width",
  "description": "Example block scaffolded with Create Block tool.",
  "supports": {
	"html": false
  },
  "attributes": {
	"pictureID": {
	  "type": "number",
	  "default": null
	},
	"pictureURL": {
	  "type": "string",
	  "source": "attribute",
	  "attribute": "src",
	  "selector": "img"
	},
	"pictureAlt": {
	  "type": "string",
	  "source": "attribute",
	  "attribute": "alt",
	  "selector": "img"
	},
	"content": {
	  "type": "string",
	  "source": "html",
	  "selector": ".content"
	}
  },
  "textdomain": "denisdumsblocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}
